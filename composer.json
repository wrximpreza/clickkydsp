{
    "name": "phpixie/project-auth",
    "type": "project",
    "description" : "PHPixie authorization skeleton project",
    "license" : "BSD",
    "autoload": {
        "psr-4": {
            "": [
                "src/",
                "tests/",

                "bundles/app/src/",
                "bundles/app/tests/"
            ]
        }
    },
    "require": {
        "phpixie/default-bundle": "~3.1",
        "phpixie/bundle-framework": "~3.1",
        "guzzlehttp/guzzle": "6.2.2",
        "paquettg/php-html-parser": "1.7.0",
        "php-curl-class/php-curl-class": "7.1.0",
        "monolog/monolog": "1.22.0"
    },
    "require-dev": {
        "phpixie/test": "~3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.*-dev"
        }
    }
}
